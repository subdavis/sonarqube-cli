{
  "name": "sonarqube-cli",
  "description": "A command-line API client for SonarQube Server and Cloud",
  "homepage": "https://github.com/subdavis/sonarqube-cli",
  "license": "MIT",
  "keywords": [
    "SonarQube",
    "SonarCloud"
  ],
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subdavis/sonarqube-cli.git"
  },
  "bin": {
    "snr": "./dist/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsx src/index.ts",
    "lint": "eslint src",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "refresh-docs": "tsx docs/refresh-docs.ts"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "chalk": "^5.6.0",
    "commander": "^12.0.0",
    "turndown": "^7.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "yarn@4.9.3"
}
